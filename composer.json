{
    "name": "chendujin/id-card-number",
    "description": "Chinese Mainland Personal ID Card Validation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "chendujin",
            "email": "thomas.chen.jobs@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chendujin\\IdCardNumber\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chendujin\\IdCardNumber\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "illuminate/config": "~5|^6|^7|^8|^9",
        "illuminate/filesystem": "~5|^6|^7|^8|^9",
        "illuminate/support": "~5|^6|^7|^8|^9",
        "illuminate/hashing": "~5|^6|^7|^8|^9",
        "illuminate/session": "~5|^6|^7|^8|^9"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chendujin\\IdCardNumber\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
